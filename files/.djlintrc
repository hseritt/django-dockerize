{
  "profile": "django",
  "extension": "html",
  "indent": 2,
  "max_line_length": 120,
  "format_attribute_template_tags": true,
  "format_css": true,
  "format_js": false,
  "blank_line_after_tag": "load,extends,endblock,endif,endfor,endwith",
  "blank_line_before_tag": "block,if,for,with",
  "preserve_leading_space": false,
  "preserve_blank_lines": true,
  "max_blank_lines": 1,
  "ignore": "H006,H030,H031,D018",
  "custom_blocks": "",
  "custom_html": "",
  "line_break_after_multiline_tag": true,
  "close_void_tags": false
}
